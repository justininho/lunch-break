; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="lunch break"
run/main_scene="res://game.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/show_image=false
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://resources/default_bus_layout.tres"

[autoload]

BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"
Global="*res://scripts/global.gd"
InteractionManager="*res://interaction/interaction_manager.tscn"
Inventory="*res://inventory/inventory.gd"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"gold_key": "res://characters/gold_key.dch"
}
directories/dtl_directory={
"examine": "res://interaction/nodes/examine/examine.dtl",
"pickup": "res://interaction/nodes/pickup/pickup.dtl",
"yes_no_timeline": "res://interaction/nodes/yes_no_timeline.dtl"
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
save/autosave_delay=60.0
save/encryption_on_exports_only=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
audio/max_channels=4.0
layout/end_behaviour=0
text/autopauses={}
glossary/default_case_sensitive=true
variables={
"examine_text": "",
"no_text": "No",
"pickup_no": "",
"pickup_text": "",
"pickup_yes": "",
"yes_no_description": "Yes or no?",
"yes_text": "Yes"
}
layout/style_list=["res://styles/default_style.tres"]
layout/default_style="res://styles/default_style.tres"

[display]

window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/phantom_camera/plugin.cfg")

[global_group]

interactable="objects that are interactable"
unlockable=""
collectible=""
examinable=""
examine_ui=""
player=""

[gui]

theme/custom="res://theme/lunch_break_theme.tres"

